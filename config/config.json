      "-silent","-nc","-json", "-o", ""
    ],
    "tlsx": [
      "-l", "",
      "-p","443",
      "-scan-mode","auto",
      "-ps",
      "-scan-all-ips",
      "-ip-version","4,6",
      "-so",
      "-tls-version",
      "-silent","-nc","-json", "-o", ""
    ],
    "katana": [
      "-list", "",
      "-js-crawl",
      "-known-files",
      "-automatic-form-fill",
      "-headless",
      "-system-chrome",
      "--no-sandbox",
      "-fields","url,path,fqdn,rdn,rurl,qurl,qpath,file,key,value,kv,dir,udir",
