      "-parallelism",10,
      "-rate-limit",150,
      "-headless-options","--disable-default-apps=true --disable-extensions=true --disable-popup-blocking=true --disable-prompt-on-repost=true --blink-settings=true --enable-quic=imagesEnabled=false --quic-version=h3-23",
      "-silent","-nc","-json", "-o", ""
    ],
    "dnsx": [
      "-l", "",
      "-aaaa",
      "-cname",
      "-ns",
      "-txt",
      "-ptr",
      "-mx",
      "-soa",
      "-axfr",
      "-caa",
      "-t",100,
      "-silent","-json", "-o", ""
